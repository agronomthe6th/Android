<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_sensors"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SensorsFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/azimuth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="59dp"
            android:layout_marginTop="132dp"
            android:layout_marginEnd="108dp"
            android:layout_marginBottom="31dp"
            android:text="accelerometerSensor"
            app:layout_constraintBottom_toTopOf="@+id/textView6"
            app:layout_constraintEnd_toStartOf="@+id/accelerometerSensor"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="58dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="101dp"
            android:text="stationarySensor"
            app:layout_constraintEnd_toStartOf="@+id/stationarySensor"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/azimuth" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="58dp"
            android:layout_marginEnd="101dp"
            android:layout_marginBottom="481dp"
            android:text="motionSensor"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/motionSensor"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/accelerometerSensor"
            android:layout_width="162dp"
            android:layout_height="21dp"
            android:layout_marginTop="132dp"
            android:layout_marginEnd="32dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/stationarySensor"
            android:layout_width="162dp"
            android:layout_height="21dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="32dp"
            android:text="stationarySensor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accelerometerSensor" />

        <TextView
            android:id="@+id/motionSensor"
            android:layout_width="162dp"
            android:layout_height="21dp"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="32dp"
            android:text="motionSensor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/stationarySensor" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>